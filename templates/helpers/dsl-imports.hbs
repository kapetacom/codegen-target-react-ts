{{#ai-context}}
    /**
    * STORM-AI: Below you'll find all the available entities. No other entities are available in this scope.
    */
    {{#kaplang-types context.spec.entities.source}}
        {{#kaplang-render this}}
            import { {{type name}} } from '../../.generated/entities/{{type name}}';
        {{/kaplang-render}}
    {{/kaplang-types}}
{{/ai-context}}


{{#ai-context}}
    /**
    * STORM-AI: Below you'll find all the available API clients. No other API clients are available in this scope.
    */
    {{#consumers-of-type 'kapeta/resource-type-rest-client'}}
        {{#kaplang-rest-methods spec.source namespace=metadata.name}}
        {{#kaplang-reader-rest-controller}}
            import { use{{controller-name this}}Client } from '../.generated/clients/{{controller-name this}}Client';
        {{/kaplang-reader-rest-controller}}
        {{/kaplang-rest-methods}}

    {{/consumers-of-type}}
{{/ai-context}}